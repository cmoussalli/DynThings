@model IEnumerable<DynThings.Data.Models.ThingCategory>
@{
    ViewBag.Title = "Thing Categories";
}
<div class="container">
    <div class="page-header">
        <h1>Thing Categories</h1>
    </div>
    <div class="row">
        <div class="col-md-6">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogThingCategoryAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtThingCategorySearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_ThingCategoryListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_ThingCategoryListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="well well-lg">
        <div id="divThingCategorysList">
        </div>
    </div>
</div>
@section scriptfiles{
    <script src="~/Assets/Scripts/DynScripts/ThingCategories.js"></script>
}
@section scripts{
    <script>
        $(document).ready(function () {
            LoadPart_ThingCategoryListDiv();
            AttachEventThingCategorysListPager();
        });

       
    </script>
}