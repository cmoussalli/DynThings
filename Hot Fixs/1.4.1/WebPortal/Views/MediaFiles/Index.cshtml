@model IEnumerable<DynThings.Data.Models.MediaFile>
@{
    ViewBag.Title = "Media File";
}
<div class="container">
    <div class="page-header">
        <h1>Media Files</h1>
    </div>
    <div class="row">
        <div class="col-md-6">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogMediaFileAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtMediaFileSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_MediaFileListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_MediaFileListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="well well-lg">
        <div id="divMediaFilesList">
        </div>
    </div>
</div>
@section scriptfiles{
    <script src="~/Assets/Scripts/DynScripts/MediaFiles.js"></script>
}
@section scripts{
    <script>
        $(document).ready(function () {
            LoadPart_MediaFileListDiv();
            AttachEventMediaFilesListPager();
        });


    </script>
}